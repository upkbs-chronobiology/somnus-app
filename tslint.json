{
  "rules": {
    "no-duplicate-variable": true,
    "no-unused-variable": [
      true
    ],
    "quotemark": [
      true,
      "single"
    ],
    "no-trailing-whitespace": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
